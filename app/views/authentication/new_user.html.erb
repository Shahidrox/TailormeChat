  <body class="login no-animation">
    <a href="/home" class="login-btn themed-background-default">
      <span class="login-logo">
        <span class="square1 themed-border-default"></span>
        <span class="square2"></span>
      </span>
    </a>
    <div id="login-container">
      <div class="block-tabs block-themed">
        <ul id="login-tabs" class="nav nav-tabs" data-toggle="tabs">
          <li class="text-center" style="width:100%" >
            <a href="#"><i class="fa fa-user"></i> Register </a>
          </li>
        </ul>
        <div class="tab-content">
          <div class="tab-pane active" id="login-form-tab">
            <div id="login-form" class="form-horizontal">
              <%= form_for @user, :as => :user, :url => new_user_path, :method => :put do |f| %>
              <div class="form-group">
                <div class="col-xs-12">
                  <div class="input-group">
                    <span class="input-group-addon"><i class="fa fa-globe fa-fw"></i></span>
                    <!--<input id="user_shop_name" name="user[shop_name]" type="text" class="form-control" placeholder="Shop url...........">-->
                    <select id="user_shop_name" name="user[shop_name]" class="form-control" size="1">
                      <option value="">Please select shop name</option>
                      <option value="tmo-sg.myshopify.com">tmo-sg.myshopify.com</option>
                      <option value="tmo-profile.myshopify.com">tmo-profile.myshopify.com</option>
                    </select>
                    <%= show_field_error(@user, :shop_name) %>
                  </div>
                </div>
              </div>
              <!--<div class="form-group">
                <div class="col-xs-12">
                  <div class="input-group">
                    <span class="input-group-addon"><i class="fa fa-adjust fa-fw"></i></span>
                    <select id="user_title" name="user[title]" class="form-control" size="1">
                      <option value="">Please select title</option>
                      <option value="Alterations">Alterations</option>
                      <option value="Sales">Sales</option>
                      <option value="General Enquires">General Enquires</option>
                    </select>
                    <%#= show_field_error(@user, :title) %>
                  </div>
                </div>
              </div>-->
              <div class="form-group">
                <div class="col-xs-12">
                  <div class="input-group">
                    <span class="input-group-addon"><i class="fa fa-user fa-fw"></i></span>
                    <input id="user_username" name="user[username]" type="text" class="form-control" placeholder="Username">
                    <%= show_field_error(@user, :username) %>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <div class="col-xs-12">
                  <div class="input-group">
                    <span class="input-group-addon"><i class="fa fa-envelope-o fa-fw"></i></span>
                    <input id="user_email" name="user[email]" type="text"  class="form-control" placeholder="Email....">
                    <%= show_field_error(@user, :email) %>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <div class="col-xs-12">
                  <div class="input-group">
                    <span class="input-group-addon"><i class="fa fa-asterisk fa-fw"></i></span>
                    <input id="user_password" name="user[password]" type="password" class="form-control" placeholder="Password..">
                    <%= show_field_error(@user, :password) %>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <div class="col-xs-12">
                  <div class="input-group">
                    <span class="input-group-addon"><i class="fa fa-asterisk fa-fw"></i></span>
                    <input id="user_password_confirmation" name="user[password_confirmation]" type="password" class="form-control" placeholder="Retype Password">
                    <%= show_field_error(@user, :password_confirmation) %>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <div class="col-xs-12 clearfix">
                  <div class="pull-right">
                    <button type="submit" class="btn btn-success remove-margin">Register</button>
                  </div>
                  <div class="pull-left">
                    <a href="/sign_in" class="remove-margin">Login</a>
                  </div>
                  <div class="pull-left login-extra-check">
                    <!--<label for="login-remember-me">
                    <div class="icheckbox_square-grey" style="position: relative;"><input type="checkbox" id="login-remember-me" name="login-remember-me" class="input-themed" style="position: absolute; opacity: 0;"><ins class="iCheck-helper" style="position: absolute; top: 0%; left: 0%; display: block; width: 100%; height: 100%; margin: 0px; padding: 0px; border: 0px; opacity: 0; background: rgb(255, 255, 255);"></ins></div>
                    Remember me
                    </label>-->
                  </div>
                </div>
              </div>
              <%end%>
            </div>
          </div>
        </div>
      </div>
    </div>
